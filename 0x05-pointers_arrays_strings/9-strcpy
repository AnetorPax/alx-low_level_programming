#include "main.h"
#include <stdio.h>
/**
* _strcpy - The function copies the string from src to dest
* @src: Pointer to the source file to be copied.
* @dest: Pointer to the destination to be copied to.
* Return: dest
*/
char *_strcpy(char *dest, char *src)
{

for (i = 0; i < n && src[i] != '\0'; i++)
dest[i] = src[i];
for ( ; i < n; i++)
dest[i] = '\0';

return dest;
}